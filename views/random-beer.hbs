<section class='random-beer'>
  <ul>
    {{#each randomBeer}}
      <div><img src={{this.image_url}} width="100" height="300" />
        <li><h3> {{this.name}} </h3>
        <p> {{this.description}} </p>
        <p> {{this.tagline}} </p>
        <p> {{this.food_pairing}} </p>
        <p> {{this.brewers_tips}}</p>
      </li>
      </div>

    {{/each}}
  </ul>
</section>